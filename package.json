{
  "name": "express-v2-users-crud",
  "version": "1.0.0",
  "main": "dist/server.js",
  "types": "dist/server.d.ts",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --respawn -r tsconfig-paths/register src/server.ts",
    "build": "tsup",
    "start": "node dist/server.js",
    "migrate": "drizzle-kit generate && drizzle-kit push",
    "compose": "docker compose up -d",
    "compose-force": "docker compose up -d --force-recreate",
    "compose-build": "docker compose build --no-cache && docker compose up -d --force-recreate",
    "down": "docker compose down --remove-orphans"
  },
  "dependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.0.12",
    "bcryptjs": "^3.0.2",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.44.2",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "pino": "^8.17.0",
    "pino-pretty": "^10.3.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.3",
    "zod": "^3.23.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/pg": "^8.15.4",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "drizzle-kit": "^0.31.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.5.0",
    "typescript": "^5.2.2"
  }
}
